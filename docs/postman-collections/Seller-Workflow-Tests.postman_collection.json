            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test('Status code is 201', function () {",
                  "    pm.expect(pm.response.code).to.equal(201);",
                  "});",
                  "",
                  "pm.test('Registration successful', function () {",
                  "    const response = pm.response.json();",
                  "    pm.expect(response.message).to.include('registered successfully');",
                  "});"
                ]
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"testseller@example.com\",\n  \"password\": \"TestPass123!\",\n  \"role\": \"Seller\",\n  \"firstName\": \"Test\",\n  \"lastName\": \"Seller\",\n  \"company\": \"Test Seller Company\",\n  \"country\": \"United States\"\n}"
